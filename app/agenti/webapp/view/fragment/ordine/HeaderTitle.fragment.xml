<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:uxap="sap.uxap"
>
    <uxap:ObjectPageDynamicHeaderTitle>
        <uxap:snappedContent>
            <ObjectStatus
                press=".onCustomerObjectPagePress"
                text="{proposta>/objectPageLayout/mode}: {proposta>/objectPageLayout/title}"
                inverted="true"
                active="true"
                icon="sap-icon://information"
                state="Indication14"
            />
        </uxap:snappedContent>
        <uxap:navigationActions>
            <core:Fragment
                fragmentName="cles.agenti.view.fragment.common.ExternalLinks"
                type="XML"
            />
        </uxap:navigationActions>
        <uxap:actions>
            <Button
                text="Segnalazioni"
                type="Critical"
                tooltip="Apre il menÃ¹ delle segnalazioni"
                icon="sap-icon://warning"
                press=".onReportPress"
            />
            <Button
                text="Allegati"
                type="Emphasized"
                tooltip="Carica allegati di testata"
                icon="sap-icon://attachment"
                press=".onHeaderAttachmentPress"
            />
        </uxap:actions>
    </uxap:ObjectPageDynamicHeaderTitle>
</core:FragmentDefinition>
