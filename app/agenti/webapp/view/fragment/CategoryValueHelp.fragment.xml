<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog title="Categorie"
        confirm=".onCategoryValueHelpConfirm"
        items="{/items}">
        <StandardListItem icon="{icon}"
            iconDensityAware="false"
            iconInset="false"
            title="{name}"
            description="{description}" />
    </SelectDialog>
</core:FragmentDefinition>