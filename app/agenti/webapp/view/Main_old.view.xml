<mvc:View
    controllerName="cles.agenti.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:layout="sap.ui.layout"
    xmlns:core="sap.ui.core"
    height="100%"
>
    <f:DynamicPage showFooter="true">
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="false">
                <core:Fragment
                    fragmentName="cles.agenti.view.fragment.main.Header"
                    type="XML"
                />
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <core:Fragment
                fragmentName="cles.agenti.view.fragment.main.Content"
                type="XML"
            />
        </f:content>
        <f:footer>
            <core:Fragment
                fragmentName="cles.agenti.view.fragment.main.Footer"
                type="XML"
            />
        </f:footer>
    </f:DynamicPage>
</mvc:View>
