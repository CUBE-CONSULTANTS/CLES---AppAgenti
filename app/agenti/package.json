{
  "name": "agenti",
  "version": "0.0.1",
  "description": "App Agenti - Ordini di vendita",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^3.0.0",
    "@sap/ux-ui5-tooling": "1"
  },
  "scripts": {
    "build": "ui5 build --clean --include-task=generateManifestBundle --dest webapp/dist",
    "build:watch": "ui5 build --watch --dest webapp/dist",
    "serve:dist": "npx http-server ./webapp/dist -p 8080 --silent",
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf"
  }
}
