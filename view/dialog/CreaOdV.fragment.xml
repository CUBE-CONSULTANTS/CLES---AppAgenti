<core:FragmentDefinition xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog title="Crea OdV" stretch="true">
        <buttons>
            <Button text="Salva come Preordine"
                enabled="{= ${/table/items/length} ? true : false }"
                type="Attention"
                press=".onCreaOdvConfirm" />
            <Button text="Salva come Offerta"
                enabled="{= ${/table/items/length} ? true : false }"
                type="Accept"
                press=".onCreaOdvConfirm" />
            <Button text="Salva come Ordine di Vendita"
                enabled="{= ${/table/items/length} ? true : false }"
                type="Accept"
                press=".onCreaOdvConfirm" />
            <Button text="Chiudi"
                type="Reject"
                press=".defaultDialogClose" />
        </buttons>
        <content>
            
        </content>
    </Dialog>
</core:FragmentDefinition>